{
  "run": [
    {
      "method": "shell.start",
      "params": {
        "path": "app",
        "venv": "env"
      }
    },
    {
      "method": "shell.enter",
      "params": {
        "message": "python app.py",
        "on": [
          {
            "event": "/(http:\\/\\/\\S+)/",
            "return": "{{event.matches[0][1]}}"
          }
        ]
      },
      "notify": true
    },
    {
      "method": "browser.open",
      "params": {
        "uri": "{{input}}",
        "target": "_blank"
      }
    },
    {
      "method": "process.wait"
    }
  ]
}
